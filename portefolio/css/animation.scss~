


/* ==================================================
    animation button menu-move
================================================== */

@-webkit-keyframes menu-move {
		0%{ 
			@include position-absolute("0","0", false, false,"10");
		}
		100%{
			@include position-absolute("0","200px", false, false,"10");
	 	}
	 } 
	@-moz-keyframes menu-move {
		0%{ 
			@include position-absolute("0","0", false, false,"10");
		}
		100%{
			@include position-absolute("0","200px", false, false,"10");
	 	}
	 } 
	@-o-keyframes menu-move{ 
		0%{ 
			@include position-absolute("0","0", false, false,"10");
		}
		100%{
			@include position-absolute("0","200px", false, false,"10");
	 	}
	 }
	@keyframes menu-move{ 
		0%{ 
			@include position-absolute("0","0", false, false,"10");
		}
		100%{
			@include position-absolute("0","200px", false, false,"10");
	 	}
	} 

